<view class="container"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled data-event-opts="{{[['tap',[['navSearch',['$event']]]]]}}" bindtap="__e"/></view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="background:rgb(203, 87, 60);"></view><swiper class="carousel" circular data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navSwiper',['$0'],[[['carouselList','',index]]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.url}}"></image></swiper-item></block></swiper><block wx:if="{{carouselList.length>0}}"><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></block></view><view class="cate-section"><view data-event-opts="{{[['tap',[['navCoupon',['$event']]]]]}}" class="cate-item" bindtap="__e"><image src="/static/temp/coupon.png"></image><text>领券中心</text></view><block wx:for="{{cates}}" wx:for-item="cate" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['navToList',['$0'],[[['cates','',__i0__,'productCateUuid']]]]]]]}}" class="cate-item" bindtap="__e"><image src="{{cate.catePicUrl}}"></image><text>{{cate.cateName}}</text></view></block></view><block wx:if="{{announcement.length>0}}"><view class="m-t"><uni-notice-bar vue-id="1" scrollable="true" showIcon="true" showClose="true" showGetMore="true" text="{{announcement[0].title}}" single="true" moreText="更多" data-event-opts="{{[['^getmore',[['navAnnouncement']]]]}}" bind:getmore="__e" bind:__l="__l"></uni-notice-bar></view></block><block wx:if="{{ads.length>0}}"><view class="ad-1"><image src="{{ads[0].url}}" mode="scaleToFill" data-event-opts="{{[['tap',[['navAD',['$0'],['ads.__$n0']]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{secKills.length>0}}"><view class="seckill-section m-t"><view class="s-header"><image class="s-img" src="/static/temp/secskill-img.jpg" mode="widthFix"></image><block wx:if="{{secKillFlag==''}}"><uni-countdown class="countdown" vue-id="2" day="{{secKillCountDown.days}}" hour="{{secKillCountDown.hours}}" minute="{{secKillCountDown.minutes}}" second="{{secKillCountDown.seconds}}" color="#FFFFFF" background-color="#333333" bind:__l="__l"></uni-countdown></block><block wx:if="{{secKillFlag=='END'}}"><text class="end">已结束</text></block><block wx:if="{{secKillFlag=='START'}}"><text class="end">秒杀中</text></block><text data-event-opts="{{[['tap',[['navSecKill']]]]}}" class="yticon icon-you" bindtap="__e"></text></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{secKills}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navSecKillDetail',['$0'],[[['secKills','',index]]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{item.productDTO.productMainImage.url}}" mode="aspectFill"></image><text class="title clamp">{{item.productDTO.productName}}</text><text class="price">{{"￥"+item.unitPrice}}</text><text></text></view></block></view></scroll-view></view></block><block wx:if="{{groupBuys.length>0}}"><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">精品团购</text><text class="tit2">Boutique Group Buying</text></view><text data-event-opts="{{[['tap',[['navGroupBuy']]]]}}" class="yticon icon-you" bindtap="__e"></text></view></block><block wx:if="{{groupBuys.length>0}}"><view class="group-section"><swiper class="g-swiper" duration="{{500}}"><block wx:for="{{groupBuys}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index%2===0}}"><swiper-item data-event-opts="{{[['tap',[['navGroupBuyDetail',['$0'],[[['groupBuys','',index]]]]]]]}}" class="g-swiper-item" bindtap="__e"><block wx:if="{{index<groupBuys.length}}"><view class="g-item left"><image src="{{groupBuys[index].productDTO.productMainImage.url}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{groupBuys[index].productDTO.productName}}</text><view class="price-box"><text class="price">{{"￥"+groupBuys[index].productDTO.unitPrice}}</text><text class="m-price">{{"￥"+groupBuys[index].productDTO.unitPriceStandard}}</text></view><view class="pro-box"><view class="progress-box"><progress percent="72" activeColor="#fa436a" active stroke-width="6"></progress></view><text>{{groupBuys[index].minUserCount+"人成团"}}</text></view></view></view></block><block wx:if="{{index+1<groupBuys.length}}"><view class="g-item right"><image src="{{groupBuys[index+1].productDTO.productMainImage.url}}" mode="aspectFill"></image><view class="t-box"><text class="title clamp">{{groupBuys[index+1].productDTO.productName}}</text><view class="price-box"><text class="price">{{"￥"+groupBuys[index+1].productDTO.unitPrice}}</text><text class="m-price">{{"￥"+groupBuys[index+1].productDTO.unitPriceStandard}}</text></view><view class="pro-box"><view class="progress-box"><progress percent="72" activeColor="#fa436a" active stroke-width="6"></progress></view><text>{{groupBuys[index+1].minUserCount+"人成团"}}</text></view></view></view></block></swiper-item></block></block></swiper></view></block><block wx:for="{{productGroups}}" wx:for-item="group" wx:for-index="__i1__"><view><view class="f-header m-t"><block wx:if="{{group.iconUrl}}"><image src="{{group.iconUrl}}"></image></block><view class="tit-box"><text class="tit">{{group.groupName}}</text><text class="tit2">{{group.groupDescription}}</text></view><text data-event-opts="{{[['tap',[['navToGroupPage',['$0'],[[['productGroups','',__i1__]]]]]]]}}" class="yticon icon-you" bindtap="__e"></text></view><view class="hot-floor"><view class="floor-img-box"><image class="floor-img" src="{{group.backgroundUrl}}" mode="scaleToFill"></image></view><scroll-view class="floor-list" scroll-x><view class="scoll-wrapper"><block wx:for="{{group.productList}}" wx:for-item="product" wx:for-index="__i2__"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['productGroups','',__i1__],['productList','',__i2__]]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{product.productMainImage.url}}" mode="aspectFill"></image><text class="title clamp">{{product.productName}}</text><text class="price">{{"￥"+product.unitPrice}}</text></view></block><view data-event-opts="{{[['tap',[['navToGroupPage',['$0'],[[['productGroups','',__i1__]]]]]]]}}" class="more" bindtap="__e"><text>查看全部</text><text>More+</text></view></view></scroll-view></view></view></block><view class="recommend-section"><view class="s-header"><text class="tit">为您推荐</text><text data-event-opts="{{[['tap',[['navFind',['$event']]]]]}}" class="yticon icon-you" bindtap="__e"></text></view><block wx:for="{{articleList}}" wx:for-item="item" wx:for-index="__i3__" wx:key="articleUuid"><view class="rec-section"><view data-event-opts="{{[['tap',[['navToDetails',['$0'],[[['articleList','articleUuid',item.articleUuid]]]]]]]}}" class="rec-item" bindtap="__e"><view class="left"><text class="title">{{item.title}}</text><view class="bot"><text class="author">{{item.tagDTO.tagName}}</text><text class="time">{{item.publishTime}}</text></view></view><view class="right"><image class="img" src="{{item.coverImageUrl}}" mode="aspectFill"></image></view></view></view></block></view></view>