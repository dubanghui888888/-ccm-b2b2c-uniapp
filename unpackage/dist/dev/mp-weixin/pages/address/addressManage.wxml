<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" maxlength="{{10}}" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','telephone','$event',[]],['addressData']]]]]}}" value="{{addressData.telephone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">省市区</text><view class="input">{{''+addressData.province+"  "+addressData.city+"  "+addressData.area+''}}</view><text data-event-opts="{{[['tap',[['showAddressRegion',['$event']]]]]}}" class="yticon icon-you" bindtap="__e"></text></view><view class="input"><w-picker class="vue-ref" vue-id="1" mode="region" defaultVal="{{defaultRegion}}" hideArea="{{false}}" timeout="{{true}}" data-ref="region" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" maxlength="{{100}}" type="text" placeholder="省市区及详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','street','$event',[]],['addressData']]]]]}}" value="{{addressData.street}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">邮政编码</text><input class="input" type="text" maxlength="{{6}}" placeholder="邮政编码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','zipcode','$event',[]],['addressData']]]]]}}" value="{{addressData.zipcode}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.default}}" color="#09A0F7" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">保存</button><block wx:if="{{manageType==='edit'}}"><button data-event-opts="{{[['tap',[['del',['$event']]]]]}}" class="del-btn" bindtap="__e">删除</button></block></view>