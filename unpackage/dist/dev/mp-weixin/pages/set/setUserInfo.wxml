<view class="container"><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}"><text class="cell-tit">手机号码</text><text class="cell-tip">{{userInfo.personalPhone}}</text></view><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['/pages/set/setUserInfoName']]]]]}}" bindtap="__e"><text class="cell-tit">昵称</text><text class="cell-tip">{{userInfo.name||'未设置'}}</text><text class="cell-more yticon icon-you"></text></view><view class="list-cell b-b m-t" hover-class="cell-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" bindtap="__e"><text class="cell-tit">头像</text><image class="cell-tip portrait" src="{{userInfo.photoUrl||'/static/missing-face.png'}}"></image><text class="cell-more yticon icon-you"></text></view></view>